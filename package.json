    "babel-core": "^4.7.0",
    "babel-jest": "^4.0.0",
    "babel-loader": "^4.2.0",
    "css-loader": "^0.14.4",
    "del": "^1.1.1",
    "file-loader": "^0.8.4",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-concat": "^2.5.2",
    "gulp-connect": "^2.2.0",
    "gulp-livereload": "^3.8.0",
    "gulp-load-plugins": "^0.9.0",
    "gulp-size": "^1.2.1",
    "gulp-stylus": "^2.0.1",
    "gulp-uglifyjs": "^0.6.1",
    "gulp-util": "^3.0.4",
    "gulp-watch": "^4.2.1",
    "gulp-webpack": "^1.3.0",
    "jest-cli": "^0.4.0",
    "object-assign": "^2.0.0",
    "react-tools": "^0.13.1",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.7.3"
  },
  "dependencies": {
    "d3": "^3.5.5",
    "font-awesome": "^4.3.0",
    "moment": "^2.10.3",
    "piwik": "^0.4.1",
    "purecss": "^0.6.0",
    "react": "^0.13.1",
    "react-router": "^0.13.2",
    "reflux": "^0.2.7",
    "reqwest": "^1.1.5"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/object-assign",
      "<rootDir>/test-utils/stubRouterContext.jsx"
